initialize   MOOS_MANUAL_OVERRIDE = false
initialize   JAIA_MISSION_STATE = PRE_DEPLOYMENT__OFF

Behavior = BHV_Waypoint
{ 
  name      = jaia_movement_transit
  pwt       = 100
  condition = JAIA_MISSION_STATE = UNDERWAY__MOVEMENT__TRANSIT

  updates    = JAIA_MOVEMENT_TRANSIT_UPDATES
  endflag    = JAIA_MOVEMENT_TRANSIT_COMPLETE = true
  
  lead = 8
  lead_damper = 1
  lead_to_start = true
  capture_line = false
  capture_radius = 5.0
  slip_radius = 15.0
  efficiency_measure = all
  perpetual = true
  
  // set by updates
  // speed = ...   // meters per second
  // point = ...
}



 Behavior = BHV_ConstantDepth
 {
   name         = jaia_fixed_depth
   pwt          = 100

   basewidth = 100          // default
   depth = 0            // default
   duration = no-time-limit
   peakwidth = 3            // default
   summitdelta = 50           // default
 }
