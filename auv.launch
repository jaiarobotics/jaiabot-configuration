#!/usr/bin/env goby_launch

# start the simulator
[kill=SIGTERM] goby_basic_frontseat_simulator $(gen/auv.py frontseat_sim)

# start the Goby processes
gobyd <(gen/auv.py gobyd)
goby_frontseat_interface_basic_simulator <(gen/auv.py goby_frontseat_interface_basic_simulator)
[env=GOBY_MOOS_GATEWAY_PLUGINS=libgoby_ivp_frontseat_moos_gateway_plugin.so] goby_moos_gateway <(gen/auv.py goby_moos_gateway) -vv
goby_liaison <(gen/auv.py goby_liaison)

[kill=SIGTERM] gen/moos_gen.sh
[kill=SIGTERM] MOOSDB /tmp/jaiabot_${jaia_auv_index}.moos
[kill=SIGTERM] pHelmIvP /tmp/jaiabot_${jaia_auv_index}.moos
[kill=SIGTERM] pMarineViewer /tmp/jaiabot_${jaia_auv_index}.moos
[kill=SIGTERM] uProcessWatch /tmp/jaiabot_${jaia_auv_index}.moos
[kill=SIGTERM] pNodeReporter /tmp/jaiabot_${jaia_auv_index}.moos
